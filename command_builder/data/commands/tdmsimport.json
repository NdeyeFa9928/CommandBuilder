{
  "name": "tdmsimport",
  "description": "Importe un ou plusieurs fichiers TDMS dans une base SQLite",
  "category": "import",
  "executable": "tdmsimport",
  "parameters": {
    "required": [
      {
        "name": "INPUT_TDMS_FILE",
        "type": "file",
        "description": "Fichier TDMS à importer",
        "validation": {
          "file_extensions": [".tdms"]
        }
      },
      {
        "name": "DATABASE_FILE",
        "type": "file",
        "description": "Fichier de base de données SQLite",
        "validation": {
          "file_extensions": [".db", ".sqlite", ".sqlite3"]
        }
      },
      {
        "name": "config",
        "flag": "--config",
        "type": "file",
        "description": "Fichier de configuration",
        "validation": {
          "file_extensions": [".json", ".yaml", ".yml"]
        }
      },
      {
        "name": "keys",
        "flag": "--keys",
        "type": "file",
        "description": "Fichier de clés de configuration"
      }
    ],
    "optional": [
      {
        "name": "offroad",
        "flag": "--offroad",
        "type": "boolean",
        "description": "Mode hors route",
        "default": false
      },
      {
        "name": "project_name",
        "flag": "--pname",
        "type": "string",
        "description": "Nom du projet"
      },
      {
        "name": "table_name",
        "flag": "--ptable",
        "type": "string",
        "description": "Nom de la table"
      },
      {
        "name": "tolerance",
        "flag": "--tol",
        "type": "float",
        "description": "Tolérance",
        "validation": {
          "min_value": 0.0
        }
      },
      {
        "name": "skip",
        "flag": "--skip",
        "type": "boolean",
        "description": "Ignorer les erreurs",
        "default": false
      },
      {
        "name": "accept_invalid",
        "flag": "--accept-invalid",
        "type": "boolean",
        "description": "Accepter les données invalides",
        "default": false
      },
      {
        "name": "imu_lag_time",
        "flag": "--imu-lag-time",
        "type": "float",
        "description": "Temps de retard IMU"
      },
      {
        "name": "image_lag_time",
        "flag": "--image-lag-time",
        "type": "float",
        "description": "Temps de retard image"
      },
      {
        "name": "gps_lag_time",
        "flag": "--gps-lag-time",
        "type": "float",
        "description": "Temps de retard GPS"
      },
      {
        "name": "imu_channel",
        "flag": "--imu-chan",
        "type": "string",
        "description": "Canal IMU"
      },
      {
        "name": "gps_channel",
        "flag": "--gps-chan",
        "type": "string",
        "description": "Canal GPS"
      }
    ]
  },
  "inputs": [
    "Fichier(s) TDMS",
    "Fichier de configuration",
    "Fichier de clés"
  ],
  "outputs": [
    "Base SQLite avec les données importées"
  ]
}
